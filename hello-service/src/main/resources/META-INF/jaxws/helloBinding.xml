<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings jaxb:version="2.1"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd ">

	<jaxb:globalBindings>
		<!-- uid should be based on the application version number not the schemas 
			used. -->
		<xjc:serializable uid="1000" />
	</jaxb:globalBindings>

	<!-- Bind common types to a nicer package name -->
	<jaxb:bindings schemaLocation="../wsdl/ct_2.0.0.xsd">
		<jaxb:schemaBindings>
			<jaxb:package name="net.trajano.ws.ct"></jaxb:package>
		</jaxb:schemaBindings>
	</jaxb:bindings>
</jaxb:bindings>
